# Firesearch Environment Variables

# OpenAI Configuration
OPENAI_API_KEY=your-openai-api-key-here

# Neo4j Configuration (for Graphiti)
NEO4J_URI=bolt://localhost:7687
NEO4J_USER=neo4j
NEO4J_PASSWORD=your-neo4j-password-here

# Firecrawl Configuration
FIRECRAWL_API_KEY=your-firecrawl-api-key-here

# Other API Keys
SERPER_API_KEY=your-serper-api-key-here
EASYOCR_API_KEY=your-easyocr-api-key-here

# Redis Configuration (for rate limiting)
UPSTASH_REDIS_REST_URL=your-upstash-redis-url-here
UPSTASH_REDIS_REST_TOKEN=your-upstash-redis-token-here

# Model Configuration (optional - defaults provided)
MODEL_NAME=gpt-4o
SMALL_MODEL_NAME=gpt-4o-mini
EMBEDDER_MODEL_NAME=text-embedding-3-large
LLM_TEMPERATURE=0.1
SEMAPHORE_LIMIT=40
MAX_REFLEXION_ITERATIONS=3 